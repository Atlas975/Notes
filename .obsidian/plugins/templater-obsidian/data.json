{
  "command_timeout": 5,
  "templates_folder": "XX-Attachments/Templates",
  "templates_pairs": [
    [
      "{{date}}",
      "echo \"$(date +%D)\""
    ],
    [
      "{{time}}",
      "echo \"$(date +\"%T\")\"\n"
    ],
    [
      "{{Title2}}",
      "echo \"$(title)\""
    ]
  ],
  "trigger_on_file_creation": true,
  "auto_jump_to_cursor": true,
  "enable_system_commands": true,
  "shell_path": "",
  "user_scripts_folder": "XX-Attachments/VaultJs",
  "enable_folder_templates": true,
  "folder_templates": [
    {
      "folder": "",
      "template": "XX-Attachments/Templates/InfoNote.md"
    }
  ],
  "syntax_highlighting": true,
  "enabled_templates_hotkeys": [
    "XX-Attachments/Templates/InfoNote.md",
    "XX-Attachments/Templates/AliasNote.md"
  ],
  "startup_templates": [
    "XX-Attachments/Templates/InfoNote.md"
  ],
  "enable_ribbon_icon": true
}